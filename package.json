{
  "name": "svelte-autobench",
  "todo-svelte": "src/index.js",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "run-p --race start:dev runCheck autoBuild watch:tailwind",
    "prepublishOnly": "npm run build",
    "autoBuild": "NODE_ENV='development' rollup -c -w",
    "runCheck": "svelte-check --watch",
    "start:dev": "sirv public --single --dev",
    "start": "",
    "watch:tailwind": "postcss public/tailwind.css -o public/global.css -w"
  },
  "devDependencies": {
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@tsconfig/svelte": "^1.0.10",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.2",
    "postcss-load-config": "^2.1.0",
    "postcss-nested": "^4.2.3",
    "postcss-selector-not": "^4.0.0",
    "rollup": "^2.26.10",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-svelte": "^6.0",
    "sirv-cli": "^1.0.6",
    "svelte": "^3.0",
    "svelte-awesome": "^2.3.0",
    "svelte-check": "^1.0.36",
    "svelte-preprocess": "^4.2.1",
    "tailwind": "^4.0.0",
    "tailwindcss": "^1.8.4",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  },
  "keywords": [
    "svelte"
  ],
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {}
