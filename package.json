{
  "name": "svelte-autobench",
  "svelte": "preprocessed/index.js",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "version": "0.0.1",
  "scripts": {
    "build": "run-p build:tailwind",
    "build:snowpack": "snowpack build",
    "preprocess": "run-s clean:preprocessed preprocess:svelte preprocess:typescript",
    "preprocess:svelte": "npx @tdi/svcli --preprocess",
    "preprocess:typescript": "tsc --declaration --outDir './preprocessed'",
    "dev": "run-p dev:checkSvelte dev:tailwind",
    "prepublishOnly": "npm run build",
    "autoBuild": "NODE_ENV='development' rollup -c -w",
    "runCheck": "svelte-check --watch",
    "start:dev": "sirv public --single --dev",
    "start": "",
    "watch:tailwind": "postcss public/tailwind.css -o public/global.css -w",
    "build:tailwind": "postcss public/tailwind.css -o public/build/global.css",
    "check": "run-p check:svelte check:typescript check:eslint",
    "check:eslint": "eslint \"./src\"",
    "check:svelte": "svelte-check --ignore preprocessed",
    "check:typescript": "tsc --noEmit",
    "clean": "run-p clean:preprocessed",
    "clean:preprocessed": "if [ -d \"./preprocessed\" ] ; then rm -r \"./preprocessed\"; fi",
    "dev:checkSvelte": "while true; do svelte-check --watch || { sleep 5; echo 'svelte-check has crashed! Will restart on next change'; inotifywait -e create -e delete -e modify -e close_write -e move  -r \"./src\"; }; done",
    "dev:tailwind": "postcss public/tailwind.css -o public/build/global.css -w",
    "preprocess:postcss": "postcss --outdir './preprocessed'"
  },
  "devDependencies": {
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@snowpack/plugin-postcss": "^1.0.7",
    "@snowpack/plugin-run-script": "^2.2.0",
    "@snowpack/plugin-svelte": "^3.2.0",
    "@snowpack/plugin-typescript": "^1.1.0",
    "@tdi/eslint-config-base": "^0.1.1",
    "@tdi/svcli": "0.0.24",
    "@tsconfig/svelte": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "mdsvex": "^0.8.8",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.4",
    "postcss-cli": "^8.2.0",
    "postcss-load-config": "^3.0.0",
    "postcss-nested": "^5.0.1",
    "postcss-selector-not": "^4.0.0",
    "rollup": "^2.33.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-svelte": "^6.1.0",
    "sirv-cli": "^1.0.8",
    "snowpack": "^2.16.1",
    "svelte": "^3.29.4",
    "svelte-as-markup-preprocessor": "^0.2.0",
    "svelte-awesome": "^2.3.0",
    "svelte-check": "^1.1.6",
    "svelte-preprocess": "^4.5.2",
    "svelte-trim": "^0.1.0",
    "tailwind": "^4.0.0",
    "tailwindcss": "^1.9.6",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  },
  "keywords": [
    "svelte"
  ],
  "files": [
    "src",
    "dist",
    "preprocessed"
  ],
  "dependencies": {},
  "types": "preprocessed/index.d.ts"
}
